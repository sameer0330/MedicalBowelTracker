<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sameerAgarwal.com.PottyTracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/poopicon_round"
        android:label="Potty Tracker"
        android:roundIcon="@mipmap/poopicon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="sameerAgarwal.com.PottyTracker.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>